# AWS Environment Variables
# Copy this file to .env.aws and update with your actual values

# Database Configuration
DB_PASSWORD=your-secure-database-password-here
RDS_ENDPOINT=your-rds-endpoint.region.rds.amazonaws.com

# Redis Configuration
REDIS_ENDPOINT=your-redis-endpoint.cache.amazonaws.com

# NFS Configuration
NFS_SERVER_IP=10.0.3.x  # Private IP of NFS server

# Application Configuration
ENVIRONMENT=production
DEBUG=false
SECRET_KEY=your-super-secret-key-for-production

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
